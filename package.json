{
  "name": "react-native-unilist",
  "version": "0.1.2",
  "description": "Beautiful FlatList and ScrollView wrapper for React Native",
  "main": "lib/module/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SamadK01/react-native-unilist.git"
  },
  "keywords": [
    "react-native",
    "flatlist",
    "scrollview",
    "list",
    "unilist"
  ],
  "author": "SamadK01",
  "license": "MIT",
  "devDependencies": {
    "react-native-builder-bob": "^0.40.8",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "scripts": {
    "prepare": "bob build",
    "lint": "echo 'No linter configured'",
    "typecheck": "tsc --noEmit",
    "test": "echo 'No tests configured'"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
